{
  "name": "@adonisjs/attachment-lite",
  "version": "1.0.1",
  "description": "Turn any field on your Lucid model to an attachment data type",
  "main": "build/providers/AttachmentLiteProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src"
  ],
  "typings": "./build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/attachment-lite"
  },
  "keywords": [
    "adonisjs",
    "lucid",
    "attachment",
    "attachment-lite"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/lucid": "^16.3.2",
    "@adonisjs/mrm-preset": "^5.0.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@poppinss/dev-utils": "^2.0.0",
    "@types/node": "^17.0.0",
    "@types/supertest": "^2.0.11",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "github-label-sync": "^2.0.2",
    "husky": "^7.0.4",
    "japa": "^4.0.0",
    "mrm": "^3.0.10",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/lucid": "^16.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/attachment-lite.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/attachment-lite/issues"
  },
  "homepage": "https://github.com/adonisjs/attachment-lite#readme",
  "dependencies": {
    "@poppinss/utils": "^4.0.0"
  },
  "adonisjs": {
    "types": "@adonisjs/attachment-lite",
    "providers": [
      "@adonisjs/attachment-lite"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
